    "url": "https://gitlab.com/tripetto/examples/react-material-ui/issues"
  },
  "scripts": {
    "start": "tripetto ./static/demo.json --tutorial",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"static/**/*.html\" \"src/**/*.less\" \"*.json\" \"!package-lock.json\" \"*.yml\" \"webpack.config.js\"",
