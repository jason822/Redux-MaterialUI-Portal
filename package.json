    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "style-loader": "^1.0.0",
    "superagent": "^5.1.0",
    "tripetto": "^1.7.1",
    "tripetto-block-checkbox": "^2.0.2",
